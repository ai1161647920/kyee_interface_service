<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- access_method -->
<mapper namespace="com.kyee.iszx.mapper.AccessMethodMapper">
    <!-- 字段映射 -->
    <resultMap id="accessMethodMap" type="com.kyee.iszx.bean.AccessMethod"></resultMap>
    
    <!-- This code was generated by TableGo tools, mark 1 begin. -->
    <!-- 表查询字段 -->
    <sql id="allColumns">
        accessMethod.id, accessMethod.access_method, accessMethod.access_other, accessMethod.access_type, accessMethod.access_claim
    </sql>
    <!-- This code was generated by TableGo tools, mark 1 end. -->
    
    <!-- 查询所有数据 -->
    <select id="findAccessMethodAll" resultMap="accessMethodMap">
        SELECT
        <include refid="allColumns" />
        FROM access_method accessMethod
    </select>
    
    <!-- 根据条件参数查询数据列表 -->
    <select id="findAccessMethodByCondition" resultMap="accessMethodMap" parameterType="map">
        SELECT
        <include refid="allColumns" />
        FROM access_method accessMethod WHERE 1 = 1
        <if test="accessMethod != null and accessMethod != ''">
            AND accessMethod.access_method LIKE CONCAT('%', #{accessMethod}, '%')
        </if>
        <if test="accessOther != null and accessOther != ''">
            AND accessMethod.access_other LIKE CONCAT('%', #{accessOther}, '%')
        </if>
        <if test="accessType != null and accessType != ''">
            AND accessMethod.access_type LIKE CONCAT('%', #{accessType}, '%')
        </if>
        <if test="accessClaim != null and accessClaim != ''">
            AND accessMethod.access_claim LIKE CONCAT('%', #{accessClaim}, '%')
        </if>
    </select>
    
    <!-- 根据主键查询数据 -->
    <select id="findAccessMethodByIds" resultMap="accessMethodMap" parameterType="list">
        SELECT
        <include refid="allColumns" />
        FROM access_method accessMethod WHERE accessMethod.id IN
        <foreach collection="list" index="index" item="id" open="(" separator="," close=")">
            #{id}
        </foreach>
    </select>
    
    <!-- 插入数据 -->
    <insert id="insertAccessMethod" parameterType="map">
        INSERT INTO access_method (
            id, access_method, access_other, access_type, access_claim
        ) VALUES (
            #{id},
            #{accessMethod},
            #{accessOther},
            #{accessType},
            #{accessClaim}
        )
    </insert>
    
    <!-- 批量插入数据 -->
    <insert id="insertAccessMethods" parameterType="list">
        INSERT INTO access_method (
            id, access_method, access_other, access_type, access_claim
        ) VALUES
        <foreach collection="list" index="index" item="item" separator=",">
            (
                #{item.id},
                #{item.accessMethod},
                #{item.accessOther},
                #{item.accessType},
                #{item.accessClaim}
            )
        </foreach>
    </insert>
    
    <!-- 修改数据 -->
    <update id="updateAccessMethod" parameterType="map">
        UPDATE access_method
        <set>
            <if test="accessMethod != null">
                access_method = #{accessMethod},
            </if>
            <if test="accessOther != null">
                access_other = #{accessOther},
            </if>
            <if test="accessType != null">
                access_type = #{accessType},
            </if>
            <if test="accessClaim != null">
                access_claim = #{accessClaim}
            </if>
        </set>
        WHERE id = #{id}
    </update>
    
    <!-- 批量修改数据 -->
    <update id="updateAccessMethods" parameterType="list">
        <foreach collection="list" index="index" item="item" separator=";">
            UPDATE access_method
            <set>
                <if test="item.accessMethod != null">
                    access_method = #{item.accessMethod},
                </if>
                <if test="item.accessOther != null">
                    access_other = #{item.accessOther},
                </if>
                <if test="item.accessType != null">
                    access_type = #{item.accessType},
                </if>
                <if test="item.accessClaim != null">
                    access_claim = #{item.accessClaim}
                </if>
            </set>
            WHERE id = #{item.id}
        </foreach>
    </update>
    
    <!-- 根据主键删除数据 -->
    <delete id="deleteAccessMethodById" parameterType="int">
        DELETE FROM access_method WHERE id = #{id}
    </delete>
    
    <!-- 根据主键批量删除数据 -->
    <delete id="deleteAccessMethodByIds" parameterType="list">
        DELETE FROM access_method WHERE id IN
        <foreach collection="list" index="index" item="id" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>