<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- ky_app_setting -->
<mapper namespace="com.kyee.iszx.mapper.KyAppSettingMapper">
    <!-- 字段映射 -->
    <resultMap id="kyAppSettingMap" type="com.kyee.iszx.bean.KyAppSetting"></resultMap>
    
    <!-- This code was generated by TableGo tools, mark 1 begin. -->
    <!-- 表查询字段 -->
    <sql id="allColumns">
        kyAppSetting.id, kyAppSetting.app_id, kyAppSetting.param_name, kyAppSetting.param_value, kyAppSetting.param_type, kyAppSetting.description, kyAppSetting.param_code
    </sql>
    <!-- This code was generated by TableGo tools, mark 1 end. -->
    
    <!-- 查询所有数据 -->
    <select id="findKyAppSettingAll" resultMap="kyAppSettingMap">
        SELECT
        <include refid="allColumns" />
        FROM ky_app_setting kyAppSetting
        WHERE kyAppSetting.param_flag = 1
    </select>
    
    <!-- 根据条件参数查询数据列表 -->
    <select id="findKyAppSettingByCondition" resultMap="kyAppSettingMap" parameterType="map">
        SELECT
        <include refid="allColumns" />
        FROM ky_app_setting kyAppSetting WHERE 1 = 1 AND kyAppSetting.param_flag = 1
        <if test="appId != null">
            AND kyAppSetting.app_id = #{appId}
        </if>
    </select>
    
    <!-- 根据主键查询数据 -->
    <select id="findKyAppSettingByIds" resultMap="kyAppSettingMap" parameterType="list">
        SELECT
        <include refid="allColumns" />
        FROM ky_app_setting kyAppSetting WHERE kyAppSetting.param_flag = 1 AND  kyAppSetting.id IN
        <foreach collection="list" index="index" item="id" open="(" separator="," close=")">
            #{id}
        </foreach>
    </select>
    
    <!-- 插入数据 -->
    <insert id="insertKyAppSetting" parameterType="map">
        INSERT INTO ky_app_setting (
            id, app_id, param_name, param_value, param_type
        ) VALUES (
            #{id},
            #{appId},
            #{paramName},
            #{paramValue},
            #{paramType}
        )
    </insert>
    
    <!-- 批量插入数据 -->
    <insert id="insertKyAppSettings" parameterType="list">
        INSERT INTO ky_app_setting (
            id, app_id, param_name, param_value, param_type
        ) VALUES
        <foreach collection="list" index="index" item="item" separator=",">
            (
                #{item.id},
                #{item.appId},
                #{item.paramName},
                #{item.paramValue},
                #{item.paramType}
            )
        </foreach>
    </insert>
    
    <!-- 修改数据 -->
    <update id="updateKyAppSetting" parameterType="map">
        UPDATE ky_app_setting
        <set>
            param_value = #{paramValue}
        </set>
        WHERE id = #{id}
    </update>
    
    <!-- 批量修改数据 -->
    <update id="updateKyAppSettingsValue" parameterType="list">
        <foreach collection="list" index="index" item="item" separator=";">
            UPDATE ky_app_setting
            <set>
                param_value = #{item.paramValue}
            </set>
            WHERE id = #{item.id}
        </foreach>
    </update>
    
    <!-- 根据主键删除数据 -->
    <delete id="deleteKyAppSettingById" parameterType="int">
        DELETE FROM ky_app_setting WHERE id = #{id}
    </delete>
    
    <!-- 根据主键批量删除数据 -->
    <delete id="deleteKyAppSettingByIds" parameterType="list">
        DELETE FROM ky_app_setting WHERE id IN
        <foreach collection="list" index="index" item="id" open="(" separator="," close=")">
            #{id}
        </foreach>
